
import java.util.Scanner;

public class Triangle7 {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n, i, j;
        System.out.print("n: ");
        n = input.nextInt();
        int pos = 0;
        for (i = 1; i <= n; i++) {
            for (j = 1; j <= i; j++) {
                pos++;
                System.out.print(pos + " ");
            }
            System.out.println();
        }
    }
}
