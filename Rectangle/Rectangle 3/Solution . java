
import java.util.Scanner;

public class Pattern3 {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n, i, j, row, col;
        System.out.print("n: ");
        n = input.nextInt();
        row = col = n;
        int pos = 0;
        for (i = 1; i <= row; i++) {
            for (j = 1; j <= col; j++) {
                pos++;
                System.out.print(pos + " ");
            }
            System.out.println();
        }
    }
}
