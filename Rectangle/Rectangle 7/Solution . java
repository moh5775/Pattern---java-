
import java.util.Scanner;

public class Pattern7 {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n, i, j, row, col;
        System.out.print("n: ");
        n = input.nextInt();
        row = col = n;

        for (i = 1; i <= row; i++) {
            int x = i;
            for (j = 1; j <= col; j++) {
                System.out.print(x + " ");
                x = x + n;
            }
            System.out.println();
        }
    }
}
